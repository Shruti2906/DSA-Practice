#include <iostream> 
#include<vector>
#include<algorithm>
//TC : O(n logn)

using namespace std;

vector<vector<int> > pairSum(vector<int> &arr, int s){
  
	vector<vector<int> > v;

	sort(arr.begin(), arr.end());
	
	int i=0;
	int j=arr.size()-1;
	
	while(i<j){

		if(arr[i]+arr[j] == s){
			vector<int> tmp;
				tmp.push_back(arr[i]);
				tmp.push_back(arr[j]);
				v.push_back(tmp);
				i++;
				j--;
		}
		else if(arr[i]+arr[j] > s){
			j--;
		}
		else if(arr[i]+arr[j] < s){
			i++;
		}
			
	}

	return v;
}

int main() {

 	vector<int> vect;
	vect.push_back(1);
	vect.push_back(2);
	vect.push_back(3);
	vect.push_back(4);
	vect.push_back(5);
 
	int s=5;
	
	vector<vector<int> > ans  ;

	ans=pairSum(vect, s);

	 for (int i = 0; i < ans.size(); i++) {
	 	for (int j=0; j<ans[i].size(); j++){
		 
            cout << ans[i][j] << " ";
		}
		cout<<"\n";
	}
	
	return 0;
}

